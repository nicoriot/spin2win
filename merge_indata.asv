function [ senddat ] = merge_indata( geomdat, setdat )
%MERGE_INDATA Summary of this function goes here
%   Detailed explanation goes here

senddat = zeros(8,length(geomdat(1,:)));

for i=1:1:7
    for k=1:1:length(geomdat(1,:))
    senddat(i,k) = geomdat(i,k);
    end
end

i = 8;

for k=2:1:3
    senddat(i,k) = setdat(k,1);
end

%insert active
senddat(i,4)setdat()

end


